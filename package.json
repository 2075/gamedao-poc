{
  "name": "gamedao",
  "version": "0.1.2",
  "private": true,
  "author": "2075@zero.io",
  "license": "Apache-2.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint src/**/*.js",
    "lint:ci": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@pinata/sdk": "^1.1.23",
    "@polkadot/api": "^4.13.1",
    "@polkadot/api-contract": "^4.13.1",
    "@polkadot/extension-dapp": "~0.38.3",
    "@polkadot/keyring": "~6.7.1",
    "@polkadot/types": "~4.13.1",
    "@polkadot/ui-keyring": "~0.79.1",
    "@polkadot/ui-settings": "~0.79.1",
    "@polkadot/util": "~6.7.1",
    "@polkadot/util-crypto": "~6.7.1",
    "@zeroio/type-definitions": "^0.0.6",
    "faker": "^5.5.3",
    "ipfs-http-client": "^50.1.2",
    "moment": "^2.29.1",
    "prop-types": "~15.7.2",
    "query-string": "~7.0.0",
    "react": "~17.0.2",
    "react-copy-to-clipboard": "~5.0.2",
    "react-dom": "~17.0.2",
    "react-scripts": "~4.0.3",
    "semantic-ui-css": "~2.4.1",
    "semantic-ui-react": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "eslint-config-semistandard": "~15.0.1",
    "eslint-config-standard": "~16.0.3",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-promise": "~5.1.0",
    "eslint-plugin-standard": "~5.0.0",
    "gh-pages": "~3.2.0",
    "react-app-rewired": "~2.1.8",
    "semistandard": "~16.0.0",
    "typescript": "^4.3.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "semi": [
        "off",
        "never"
      ],
      "no-extra-semi": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://gamedao.co",
  "bugs": {
    "url": "https://github.com/gamedaoco/gamedao-poc/issues"
  },
  "keywords": [
    "substrate",
    "subzero",
    "playzero",
    "gamedao"
  ]
}
