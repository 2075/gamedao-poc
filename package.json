{
  "name": "gamedao",
  "version": "0.1.2",
  "private": true,
  "author": "ZERO.IO <play@zero.io>",
  "license": "Apache-2.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint src/**/*.js",
    "lint:ci": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@polkadot/api": "^4.0.3",
    "@polkadot/api-contract": "^4.0.3",
    "@polkadot/extension-dapp": "~0.37.2",
    "@polkadot/keyring": "~6.0.5",
    "@polkadot/types": "~4.0.3",
    "@polkadot/ui-keyring": "~0.71.3",
    "@polkadot/ui-settings": "~0.71.3",
    "@polkadot/util": "~6.0.5",
    "@polkadot/util-crypto": "~6.0.5",
    "ipfs-http-client": "^49.0.4",
    "prop-types": "~15.7.2",
    "query-string": "~6.14.1",w
    "react": "~17.0.1",
    "react-copy-to-clipboard": "~5.0.2",
    "react-dom": "~17.0.1",
    "react-scripts": "~4.0.3",
    "semantic-ui-css": "~2.4.1",
    "semantic-ui-react": "^2.0.0"
  },
  "devDependencies": {
    "eslint-config-semistandard": "~15.0.1",
    "eslint-config-standard": "~16.0.2",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-promise": "~4.3.1",
    "eslint-plugin-standard": "~5.0.0",
    "gh-pages": "~3.1.0",
    "react-app-rewired": "~2.1.8",
    "semistandard": "~16.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "semistandard"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "no-extra-semi": 1
    }
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://gamedao.co",
  "bugs": {
    "url": "https://github.com/gamedaoco/gamedao-poc/issues"
  },
  "keywords": [
    "substrate",
    "subzero",
    "playzero",
    "gamedao"
  ]
}
