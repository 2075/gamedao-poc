		"markdown-it": "^12.2.0",
		"material-ui-image": "^3.3.2",
		"moment": "^2.29.1",
		"nprogress": "^0.2.0",
		"numeral": "2.0.6",
		"prettier": "^2.5.1",
		"prop-types": "~15.7.2",
		"query-string": "~7.0.1",
